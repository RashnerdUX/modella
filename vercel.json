{
  "version": 2,
  "functions": {
    "src/modella/wsgi.py": {
      "runtime": "python3.12",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "src/modella/wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": "src",
    "DJANGO_SETTINGS_MODULE": "modella.settings"
  },
  "installCommand": "pip install -r requirements-production.txt",
  "buildCommand": "cd src && python manage.py collectstatic --noinput"
}
